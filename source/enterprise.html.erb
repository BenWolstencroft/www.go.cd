          </div>
        
        
          <div class="content-card-three-column">
            <div class="content-card">
              <h3 class="text-center content-card-title">Continuous Delivery at Scale</h3>
              <%= image_tag "campaign/enterprise/postgres.svg", {:title => "Continuous Delivery at Scale", :alt => "CD illustration", :class => 'content-card_img'} %>
